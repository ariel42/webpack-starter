{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:marharyta/webpack-boilerplate.git",
  "author": "Margarita Obraztsova <marharyta.obraztsova@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "SET NODE_ENV=development&& webpack-dev-server --config webpack.config.js --mode development --hot",
    "build-stage-1": "webpack --config webpack.config.js --mode production",
    "build-stage-2": "webpack --config webpack.config.js --mode production --stage=legacy",
    "build": "SET NODE_ENV=production&& npm run build-stage-1&& npm run build-stage-2",
    "build-dev": "SET NODE_ENV=development&& webpack --config webpack.config.js --mode development"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "autoprefixer": "^9.4.3",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.0.2",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.1",
    "html-webpack-plugin": "^3.2.0",
    "ignore-loader": "^0.1.2",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.3.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "@babel/runtime-corejs3": "^7.4.2",
    "clean-webpack-plugin": "^1.0.0",
    "core-js": "^3.0.0",
    "regenerator-runtime": "^0.13.2",
    "webpack-node-externals": "^1.7.2"
  }
}
