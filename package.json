{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ariel42/webpack-starter.git"
  },
  "author": "Ariel <ariel42dev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "SET NODE_ENV=development&& webpack-dev-server --config webpack.config.js --mode development --hot",
    "build": "SET NODE_ENV=production&& npm run build-stage1-es6&& npm run build-stage2-es5",
    "build-stage1-es6": "webpack --config webpack.config.js --mode production --stage=1 --es=6",
    "build-stage2-es5": "webpack --config webpack.config.js --mode production --stage=2",
    "build-es5-only": "SET NODE_ENV=production&& webpack --config webpack.config.js --mode production",
    "build-es6-only": "SET NODE_ENV=production&& webpack --config webpack.config.js --mode production --es=6",
    "build-dev": "SET NODE_ENV=development&& webpack --config webpack.config.js --mode development",
    "postbuild-stage2-es5": "shx rm -f build/temp.html"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.0.2",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "ignore-loader": "^0.1.2",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.11.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^2.0.0",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "shx": "^0.3.2",
    "stylelint": "^9.3.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "clean-webpack-plugin": "^1.0.0",
    "core-js": "^3.0.1",
    "regenerator-runtime": "^0.13.2",
    "style-loader": "^0.23.1",
    "webpack-node-externals": "^1.7.2",
    "whatwg-fetch": "^3.0.0"
  }
}
