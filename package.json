{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ariel42/webpack-starter.git"
  },
  "author": "Ariel <ariel42dev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "SET NODE_ENV=development&& webpack-dev-server --config webpack.config.js --mode development --hot",
    "build": "SET NODE_ENV=production&& npm run build-stage1-es6&& npm run build-stage2-es5",
    "build-stage1-es6": "webpack --config webpack.config.js --mode production --stage=1 --es=6",
    "build-stage2-es5": "webpack --config webpack.config.js --mode production --stage=2",
    "build-es5-only": "SET NODE_ENV=production&& webpack --config webpack.config.js --mode production",
    "build-es6-only": "SET NODE_ENV=production&& webpack --config webpack.config.js --mode production --es=6",
    "build-dev": "SET NODE_ENV=development&& webpack --config webpack.config.js --mode development"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "autoprefixer": "^9.4.3",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.0.2",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.3.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "clean-webpack-plugin": "^1.0.0",
    "core-js": "^3.0.0",
    "regenerator-runtime": "^0.13.2",
    "webpack-node-externals": "^1.7.2",
    "whatwg-fetch": "^3.0.0"
  }
}
